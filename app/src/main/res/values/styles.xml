<resources>

    <!--
        Base application theme, dependent on SendInterface level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light">
        <!--
            Theme customizations available in newer SendInterface levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular SendInterface-level can go here. -->
    </style>
    <style name="windowAnim">
        <item name="android:windowEnterAnimation">@anim/enter_anim</item>
        <item name="android:windowExitAnimation">@anim/exit_anim</item>
    </style>
    <style name="ErrorTheme" parent="Base.Theme.AppCompat">
        <!--顶部通知栏颜色-->
        <item name="colorPrimaryDark">#FF000000</item>
        <!--顶部导航栏颜色-->
        <item name="colorPrimary">#FF000000</item>
        <!--底部导航栏颜色-->
        <item name="android:navigationBarColor">#FF000000</item>
        <!-- 默认颜色 -->
        <item name="colorAccent">#FF000000</item>
	</style>
</resources>
